{
  "db": "mongodb://localhost:27017/tireke",
  "log4js": {
    "configure": {
      "appenders": [
        {
          "category": "system",
          "type": "file",
          "filename": "logs/system.log"
        },
        {
          "category": "access",
          "type": "file",
          "filename": "logs/access.log"
        },
        {
          "category": "error",
          "type": "file",
          "filename": "logs/error.log"
        }
      ],
      "levels": {
        "system": "DEBUG",
        "access": "DEBUG",
        "error": "DEBUG"
      },
      "replaceConsole": true
    }
  },
  "reportServer": {
	  "username": "jasperadmin",
	  "password": "jasperadmin",
	  "url": "http://localhost:8080/jasperserver"
  }
}
